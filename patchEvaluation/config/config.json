{
  "system": {
    "environment": "production",
    "debugMode": false,
    "logLevel": "info"
  },
  "llm": {
    "provider": "openai",
    "maxTokens": 4000,
    "timeout": 30000
  },
  "gemini": {
    "model": "gemini-1.5-pro",
    "maxTokens": 4000,
    "temperature": 0.1,
    "timeout": 30000
  },
  "openai": {
    "model": "gpt-5",
    "maxTokens": 4000,
    "timeout": 30000
  },
  "fileOperations": {
    "maxFileSize": 52428800
  },
  "paths": {
    "promptDir": "/app/src/prompts",
    "outputDir": "/app/output",
    "logsDir": "/app/logs",
    "backupDir": "/app/backups"
  }
}
