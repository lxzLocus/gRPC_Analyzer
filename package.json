    "llamaindex": "^0.9.11",
    "lodash": "^4.17.21",
    "openai": "^4.104.0",
    "protobufjs": "^7.4.0",
    "simple-git": "^3.27.0",
    "tar": "^7.4.3",
    "tar-stream": "^3.1.7",
    "ts-morph": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
